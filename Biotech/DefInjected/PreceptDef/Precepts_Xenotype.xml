  <PreferredXenotype.comps.1.thoughtStageDescriptions.0>Xenótipo preferido</PreferredXenotype.comps.1.thoughtStageDescriptions.0>
  
  <!-- EN: Disliked xenotype -->
  <PreferredXenotype.comps.1.thoughtStageDescriptions.1>Xenótipo indesejado</PreferredXenotype.comps.1.thoughtStageDescriptions.1>
  
  <!-- EN: All preferred xenotypes -->
  <PreferredXenotype.comps.2.thoughtStageDescriptions.0>Todos os xenótipos preferidos</PreferredXenotype.comps.2.thoughtStageDescriptions.0>
  
  <!-- EN: Some disliked xenotypes -->
  <PreferredXenotype.comps.2.thoughtStageDescriptions.1>Alguns xenótipos indesejados</PreferredXenotype.comps.2.thoughtStageDescriptions.1>
  
  <!-- EN: Many disliked xenotypes -->
  <PreferredXenotype.comps.2.thoughtStageDescriptions.2>Muitos xenótipos indesejados</PreferredXenotype.comps.2.thoughtStageDescriptions.2>
  
